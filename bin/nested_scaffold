#!/usr/bin/env ruby

require 'nested_scaffold_gem'

#angenommene Variablen, die an nested_scaffold_gem.rb weiter gegeben wird
Nested_scaffold.resources(ARGV[0],ARGV[1]) 

#Ausgabe der Nested_scaffold Klasse mit Ihren Methoden
puts "Ihre erste Resource lautet: " + Nested_scaffold.erste
puts "Ihre zweite Resource lautet: " + Nested_scaffold.zweite